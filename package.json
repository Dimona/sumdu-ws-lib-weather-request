{
  "name": "@workshop/lib-nest-weather-request",
  "version": "0.1.3",
  "description": "Workshop Weather Request library",
  "author": "Dmytro Ieremenko",
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "prepare": "npm run build",
    "co:login": "aws codeartifact login --tool npm --repository workshop --domain sumdu-ws --domain-owner 316316143441 --region eu-central-1",
    "build": "rimraf dist && tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "preversion": "npm run lint",
    "version": "npm run format",
    "prepublishOnly": "npm run lint && npm run format && npm run test && npm run build"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "nestjs",
    "aws",
    "dynamodb"
  ],
  "dependencies": {
    "@nestjs/common": "^9.2.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/platform-express": "^9.2.1",
    "@typedorm/core": "^1.15.2",
    "@typedorm/common": "^1.15.2",
    "@typedorm/document-client": "^1.15.2",
    "@workshop/lib-nest-aws": "0.4.0",
    "dayjs": "^1.11.7",
    "@faker-js/faker": "^7.6.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.6.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.2.1",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.11",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "eslint": "^8.29.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "prettier": "^2.8.0",
    "supertest": "^6.3.2",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1",
    "tsconfig-paths-jest": "^0.0.1",
    "typescript": "^4.9.3"
  },
  "jest": {
    "preset": "ts-jest",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "modulePaths": [
      "<rootDir>"
    ],
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "setupFiles": [
      "./jest-setup-file.ts"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "fixtures",
      "<rootDir>/node_modules",
      "<rootDir>/.build",
      "<rootDir>/.clinic",
      "<rootDir>/.serverless",
      "<rootDir>/coverage",
      "<rootDir>/gen",
      "<rootDir>/src/main.ts",
      "<rootDir>/src/index.ts",
      "<rootDir>/src/app/app.ts",
      "<rootDir>/src/docs",
      "<rootDir>/src/aws",
      "mocks",
      "interfaces",
      "types",
      "config",
      ".module.ts$",
      ".entity.ts$",
      ".aliases.ts$"
    ]
  },
  "engines": {
    "node": "^16.16.0"
  }
}
